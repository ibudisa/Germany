@using System.Web.Optimization
@model IEnumerable<DAL.FileData>



@section scripts {

    <script src="~/Scripts/jquery-1.8.2.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>


    <script type="text/javascript">

        $(document).ready(function () {

            $('#xmltable').dataTable();

        })
    </script>

}

@{
    ViewBag.Title = "Index";
}


<table data-toggle="table" id="xmltable" class="table table-striped table-bordered table-condensed table-hover table table-responsive" data-pagination="true" data-search="true">
    <thead>
        <tr>
            <th data-field="ID" data-align="right" data-sortable="true">ID</th>
            <th data-field="NodeID" data-align="center" data-sortable="true">NodeID</th>
            <th data-field="NodeValue" data-sortable="true">NodeValue</th>
            <th data-field="Xpath" data-sortable="true">Xpath</th>
            <th data-field="SourceFile" data-sortable="true">SourceFile</th>
            <th data-field="ExpectedFile" data-sortable="true">ExpectedFile</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>

                      @Html.DisplayFor(modelItem => item.ID)
                   
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NodeID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NodeValue)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Xpath)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SourceFile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ExpectedFile)
                </td>


            </tr>
        }
    </tbody>

</table>


</br>


